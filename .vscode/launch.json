{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/dist/server.js"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "port": 9228,
      "skipFiles": ["${workspaceRoot}/node_modules/**/*.js"],
      "restart": true,
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/dist/*.js"]
    }
  ],
  "compounds": []
}
