{
  "name": "urligram",
  "version": "0.0.1",
  "author": "Urligram.com",
  "description": "Starter kit for universal sites using react, redux and typescript",
  "main": "dist/server.js",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "express",
    "jwt",
    "universal",
    "hmr"
  ],
  "scripts": {
    "dev": "webpack --watch --config ./webpack/webpack.server.dev.ts & node --inspect=9228 ./dist/server.js",
    "stage": "webpack --config ./webpack/webpack.client.stage.ts & webpack --config ./webpack/webpack.server.stage.ts",
    "prod": "webpack --config ./webpack/webpack.client.prod.ts & webpack --config ./webpack/webpack.server.prod.ts",
    "build:server:dev": "webpack --config ./webpack/webpack.server.dev.ts",
    "build:client:dev": "webpack --config ./webpack/webpack.client.dev.ts",
    "build:client:prod": "webpack --config ./webpack/webpack.client.prod.ts",
    "build:server:prod": "webpack --config ./webpack/webpack.server.prod.ts",
    "build:server:stage": "webpack --config ./webpack/webpack.server.stage.ts",
    "build:client:stage": "webpack --config ./webpack/webpack.client.stage.ts",
    "run:server": "node --inspect=9228 ./dist/server.js",
    "check": "npm run lint && npm run unused-exports",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "unused-exports": "ts-unused-exports tsconfig.json --excludePathsFromReport='vendor;stories;tools'",
    "test": "jest --no-cache --passWithNoTests",
    "test:watch": "jest --watch --no-cache --passWithNoTests",
    "test:precommit": "jest --no-cache --passWithNoTests",
    "storybook": "storybook dev -s -p 6006 watch-css"
  },
