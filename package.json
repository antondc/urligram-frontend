  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "concurrently \"npm run buildDev\" \"npm run server\"",
    "prod": "\"npm run build\"",
    "debug": "concurrently \"npm run buildDebug\" \"npm run debugServer\" ",
    "buildDev": "webpack --watch --config webpack.dev.js",
    "build": "webpack -p --config webpack.prod.js",
    "buildDebug": "webpack --watch --config webpack.dev.js",
    "server": "nodemon ./dist/server.js",
    "debugServer": "nodemon --inspect=9228 ./dist/server.js",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "jest": {
    "testRegex": "./src/shared/*/.*test.js$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/src/shared/tools/jest/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/shared/tools/jest/styleMock.js"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --config ./.eslintrc.js",
      "git add"
    ],
    "*.less": [
      "stylelint --config ./.stylelintrc.js",
      "git add"
    ]
  },
